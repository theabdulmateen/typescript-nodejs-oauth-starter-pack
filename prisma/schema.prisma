datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id             Int     @id @default(autoincrement())
  email          String  @unique
  hashedPassword String?
  srn            String? @unique
  fistName       String?
  lastName       String?
  googleId       String? @unique
  facebookId     String? @unique
}
